{
  "$id": "ce_event",
  "$schema": "https://schema.yext.com/config/km/field-presentation/v1",
  "entityType": "event",
  "displayName": "Event",
  "presentationEntries": [
    {
      "displayName": "Content",
      "entryType": "DIVIDER"
    },
    {
      "displayName": "Core Information",
      "entryType": "SECTION",
      "fields": [
        {
          "id": "name",
          "visibleOnAdd": true
        },
        {
          "id": "categories",
          "visibleOnAdd": true
        },
        {
          "id": "linkedLocation"
        },
        {
          "id": "venueName"
        },
        {
          "id": "address",
          "visibleOnAdd": true
        },
        {
          "id": "timezone"
        },
        {
          "id": "time",
          "visibleOnAdd": true
        },
        {
          "id": "description"
        },
        {
          "id": "websiteUrl"
        },
        {
          "id": "bizzabo_coverPhoto"
        },
        {
          "id": "bizzabo_headerLogo"
        },
        {
          "id": "entityId",
          "visibleOnAdd": true
        }
      ]
    },
    {
      "defaultCollapsed": true,
      "displayName": "Additional Information",
      "entryType": "SECTION",
      "fields": [
        {
          "id": "additionalPromotingLocations"
        },
        {
          "id": "performers"
        },
        {
          "id": "ageRange"
        },
        {
          "id": "organizerName"
        },
        {
          "id": "organizerEmail"
        },
        {
          "id": "organizerPhone"
        },
        {
          "id": "what3WordsAddress"
        },
        {
          "id": "attendance"
        }
      ]
    },
    {
      "displayName": "Media",
      "entryType": "SECTION",
      "fields": [
        {
          "id": "photoGallery"
        }
      ]
    },
    {
      "defaultCollapsed": true,
      "displayName": "Additional Attributes",
      "entryType": "SECTION",
      "fields": [
        {
          "id": "keywords"
        }
      ]
    },
    {
      "defaultCollapsed": true,
      "displayName": "Landing Pages",
      "entryType": "SECTION",
      "fields": [
        {
          "id": "landingPageUrl"
        }
      ]
    },
    {
      "displayName": "Other",
      "entryType": "DIVIDER"
    },
    {
      "defaultCollapsed": true,
      "displayName": "Internal Use Only",
      "entryType": "SECTION",
      "fields": [
        {
          "id": "labels"
        },
        {
          "id": "folder"
        },
        {
          "id": "deliverListingsWithoutGeocode"
        },
        {
          "id": "eventStatus"
        },
        {
          "id": "isFreeEvent"
        },
        {
          "id": "isTicketedEvent"
        },
        {
          "id": "ticketUrl"
        },
        {
          "id": "ticketAvailability"
        },
        {
          "id": "ticketPriceRange"
        },
        {
          "id": "ticketSaleDateTime"
        }
      ]
    }
  ]
}